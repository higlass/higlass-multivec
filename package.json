    "babel-loader": "^8.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "=1.0.0-rc.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "file-loader": "^1.1.11",
    "higlass": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "jasmine": "^2.99.0",
    "karma": "^4.4.1",
